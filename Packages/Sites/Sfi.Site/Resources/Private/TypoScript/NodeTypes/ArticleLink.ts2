prototype(S:ArticleLink) {
	templatePath = 'resource://Sfi.Site/Private/TypoScript/NodeTypes/ArticleLink.html'
	@process.convertUris = TYPO3.Neos:ConvertUris
	title = ${String.stripTags(node.properties.title)}
	imageUri = ImageUri {
		asset = ${q(node).property('image')}
		maximumWidth = 600
		maximumHeight = 400
		allowCropping = TRUE
	}
}
